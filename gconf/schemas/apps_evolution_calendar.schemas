      <locale name="ru">
        <short>Минута начала рабочего дня</short>
        <long>Минуты начала рабочего дня, от 0 до 59</long>
      </locale>
    </schema>

    <schema>
      <key>/schemas/apps/evolution/calendar/display/day_end_hour</key>
      <applyto>/apps/evolution/calendar/display/day_end_hour</applyto>
      <owner>evolution-calendar</owner>
      <type>int</type>
      <default>17</default>
      <locale name="C">
        <short>Workday end hour</short>
