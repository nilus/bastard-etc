      <owner>evolution-calendar</owner>
      <type>int</type>
      <default>9</default>
      <locale name="C">
        <short>Workday start hour</short>
        <long>Hour the workday starts on, in twenty four hour format, 0 to 23.</long>
      </locale>

      <locale name="ru">
        <short>Час начала рабочего дня</short>
        <long>Час начала рабочего дня, в 24-часовом формате, от 0 до 23.</long>
